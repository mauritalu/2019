
@{
    Layout = null;
}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table cellpadding="2" cellspacing="2" border="1">
        <tr>
            <th>Id</th>
            <th>Liik</th>
            <th>Nimi</th>
            <th>Kaal</th>
        </tr>
        @foreach (var cat in ViewBag.listCat)
        {
            <tr>
                <td>@cat.id</td>
                <td>@cat.Liik</td>
                <td>@cat.Nimi</td>
                <td>@cat.Kaal</td>
            </tr>
        }
    </table>

</body>
</html>
